            "actionArgs" : {
              "filename": "MIDRC_Series_table.json",
              "rootPath": "gen3_discovery"
            }
          },
          {
            "enabled": true,
            "type": "data-csv",
            "title": "CSV",
            "leftIcon": "datafile",
            "rightIcon": "download",
            "action": "data-csv",
            "actionArgs" : {
              "filename": "data_table.csv"
            }
          }
