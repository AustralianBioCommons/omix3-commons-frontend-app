[
  {
    "tabTitle": "Cases",
    "charts": {
      "baseline_year": {
        "chartType": "horizontalStacked",
        "title": "baseline_year"
      }
    },
    "filters": {
      "tabs": [
        {
          "title": "Study Selection",
          "fields": [
            "project_id"
          ]
        },
        {
          "title": "Subjects",
          "fields": [
            "baseline_year"
